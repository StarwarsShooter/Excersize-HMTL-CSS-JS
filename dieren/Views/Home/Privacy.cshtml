@{
    ViewData["Title"] = "Dieren";
}
<h1>@ViewData["Title"]</h1>

<p>Alle dieren die wij hebben</p>
<h3>Honden</h3>
<table>
    <thead>
    <tr>
        <th>Naam</th>
        <th>Leeftijd</th>
        <th>Reg. Nr.</th>
        <th>Gereserveerd</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <th>Henry</th>
        <td>2 yaar</td>
        <td>352</td>
        <td>Nee</td>
    </tr>
    <tr>
        <th>Frank</th>
        <td>8 maanden</td>
        <td>776</td>
